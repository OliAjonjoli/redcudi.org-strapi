{
  "kind": "collectionType",
  "collectionName": "businesses",
  "info": {
    "singularName": "business",
    "pluralName": "businesses",
    "displayName": "Business",
    "description": "Business, clinic, or organization model"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "bio": {
      "type": "richtext"
    },
    "lgbtq_friendly_statement": {
      "type": "richtext"
    },
    "services": {
      "type": "component",
      "component": "shared.service-item",
      "repeatable": true
    },
    "location_city": {
      "type": "string"
    },
    "location_state": {
      "type": "string"
    },
    "contact_email": {
      "type": "email"
    },
    "contact_phone": {
      "type": "string"
    },
    "contact_whatsapp": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "social_media": {
      "type": "component",
      "component": "shared.social-links",
      "repeatable": false
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "pricing_model": {
      "type": "enumeration",
      "enum": [
        "free",
        "low_cost",
        "sliding_scale",
        "standard"
      ]
    },
    "verified": {
      "type": "boolean",
      "default": false
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "specializations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::specialization.specialization",
      "inversedBy": "businesses"
    }
  }
}
