{
  "kind": "collectionType",
  "collectionName": "professionals",
  "info": {
    "singularName": "professional",
    "pluralName": "professionals",
    "displayName": "Professional"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "entity_type": {
      "type": "enumeration",
      "enum": [
        "individual_health",
        "individual_other",
        "organization"
      ],
      "required": true,
      "default": "individual_health"
    },
    "pronouns": {
      "type": "component",
      "component": "shared.pronoun",
      "repeatable": true
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bio": {
      "type": "richtext"
    },
    "lgbtq_friendly_statement": {
      "type": "richtext"
    },
    "services": {
      "type": "component",
      "component": "shared.service-item",
      "repeatable": true
    },
    "location_city": {
      "type": "string"
    },
    "location_state": {
      "type": "string"
    },
    "contact_email": {
      "type": "email"
    },
    "contact_phone": {
      "type": "string"
    },
    "contact_whatsapp": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "social_media": {
      "type": "component",
      "component": "shared.social-links",
      "repeatable": false
    },
    "pricing_model": {
      "type": "enumeration",
      "enum": [
        "free",
        "low_cost",
        "sliding_scale",
        "standard"
      ]
    },
    "verified": {
      "type": "boolean",
      "default": false
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "specializations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::specialization.specialization",
      "inversedBy": "professionals"
    },
    "professions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::profession.profession",
      "inversedBy": "professionals"
    }
  }
}
