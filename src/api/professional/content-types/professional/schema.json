{
  "kind": "collectionType",
  "collectionName": "professionals",
  "info": {
    "singularName": "professional",
    "pluralName": "professionals",
    "displayName": "Professional",
    "description": "Professional model"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "lastName": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "enumeration",
      "enum": [
        "Dr.",
        "Ing.",
        "Mtro.",
        "C.P.",
        "Lic.",
        "Psic."
      ],
      "description": "Professional title"
    },
    "slug": {
      "type": "uid"
    },
    "entity_type": {
      "type": "enumeration",
      "enum": [
        "individual_health",
        "individual_other"
      ],
      "required": true,
      "default": "individual_health"
    },
    "pronouns": {
      "type": "component",
      "component": "shared.pronoun",
      "repeatable": true
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bio": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "lgbtq_friendly_statement": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "services": {
      "type": "component",
      "component": "shared.service-item",
      "repeatable": true
    },
    "location_city": {
      "type": "string"
    },
    "location_state": {
      "type": "string"
    },
    "contact_email": {
      "type": "email"
    },
    "contact_phone": {
      "type": "string"
    },
    "contact_whatsapp": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "social_media": {
      "type": "component",
      "component": "shared.social-links",
      "repeatable": false
    },
    "pricing_model": {
      "type": "enumeration",
      "enum": [
        "free",
        "low_cost",
        "sliding_scale",
        "standard"
      ]
    },
    "verified": {
      "type": "boolean",
      "default": false
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "specializations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::specialization.specialization",
      "mappedBy": "professionals"
    },
    "professions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::profession.profession",
      "mappedBy": "professionals"
    }
  }
}
