{
  "kind": "collectionType",
  "collectionName": "health_specializations",
  "info": {
    "singularName": "health-specialization",
    "pluralName": "health-specializations",
    "displayName": "Health Specialization"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "richtext"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "health_professionals": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::health-professional.health-professional",
      "mappedBy": "specializations"
    }
  }
}
